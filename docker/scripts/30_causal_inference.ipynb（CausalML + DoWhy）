    "import dowhy\n",
    "import causalml\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "from pathlib import Path\n",
    "import yaml\n",
    "\n",
    "# å› æœé“¾: mtLeakScore â†’ nonCanonicalPyroScore â†’ Treg/MDSC â†’ Tç»†èƒåŠŸèƒ½æŠ‘åˆ¶ â†’ é¢„åå·®\n",
    "def causal_pipeline():\n",
    "    # åŠ è½½æ•°æ®\n",
    "    deconv_files = Path('data_processed/deconvolution').glob('*.rds')\n",
    "    \n",
    "    results = []\n",
    "    for file in deconv_files:\n",
    "        df = pd.read_csv(file)  # å‡è®¾å·²è½¬ä¸ºCSV\n",
    "        \n",
    "        # 1. DoWhyå› æœå›¾\n",
    "        model = dowhy.CausalModel(\n",
    "            data=df,\n",
    "            treatment='mtLeakScore',\n",
    "            outcome='CD8_T_exhausted',\n",
    "            common_causes=['Treg', 'MDSC']\n",
    "        )\n",
    "        \n",
    "        causal_estimate = model.estimate_effect(\n",
    "            method_name='backdoor.propensity_score_matching'\n",
    "        )\n",
    "        \n",
    "        results.append({\n",
    "            'sample': file.name,\n",
    "            'ate': causal_estimate.value,\n",
    "            'pvalue': causal_estimate.p_value\n",
    "        })\n",
    "    \n",
    "    pd.DataFrame(results).to_csv('results/tables/causal_results.csv')\n",
